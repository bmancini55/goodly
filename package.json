{
  "name": "goodly",
  "version": "0.3.4",
  "description": "Microservice framework",
  "author": "Brian <bmancini@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --source-maps --out-dir dist ",
    "prepublish": "npm run build",
    "watch": "nodemon --ignore dist --exec \"npm run build\" src",
    "test": "babel-node node_modules/.bin/_mocha test/unit/ test/acceptance/",
    "test:file": "babel-node node_modules/.bin/_mocha",
    "test:ci": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --report lcovonly -- test/unit/ test/acceptance/",
    "test:coverage": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --reporter nyan test/unit/ test/acceptance/"
  },
  "repository": "bmancini55/goodly",
  "keywords": [
    "microservice",
    "framework",
    "event",
    "orchestration",
    "rabbitmq",
    "amqp"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "amqplib": "^0.4.1",
    "babel-regenerator-runtime": "^6.5.0",
    "debug": "^2.2.0",
    "source-map-support": "^0.4.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.3"
  }
}
