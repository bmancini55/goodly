{
  "name": "goodly",
  "version": "0.2.0",
  "description": "Microservice framework",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --source-maps --out-dir dist ",
    "prepublish": "npm run build",
    "watch": "nodemon --ignore dist --exec \"npm run build\" src",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --compilers js:babel-register --require babel-regenerator-runtime test/unit",
    "test:integration": "mocha --compilers js:babel-register --require babel-regenerator-runtime test/integration"
  },
  "repository": "bmancini55/goodly",
  "keywords": [
    "microservice",
    "framework",
    "event",
    "orchestration",
    "rabbitmq",
    "amqp"
  ],
  "author": "Brian <bmancini@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.4.1",
    "babel-regenerator-runtime": "^6.5.0",
    "debug": "^2.2.0",
    "node-uuid": "^1.4.7",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.3"
  }
}
